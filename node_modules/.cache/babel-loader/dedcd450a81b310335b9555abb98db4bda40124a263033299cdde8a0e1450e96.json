for=\"(stat, index) in stats\" :key=\"index\" class=\"text-center\">\n          <div class=\"text-4xl font-bold text-secondary mb-2\">{{ stat.value }}</div>\n          <div class=\"text-white/60\">{{ stat.label }}</div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script setup>\nimport { ref } from 'vue';\n\n// Static testimonials data\nconst testimonials = ref([\n  {\n    content: 'Proses pembelian rumah sangat mudah dan transparan. Tim sangat profesional dan membantu dari awal hingga akad.',\n    name: 'Budi Santoso',\n    role: 'Pengusaha',\n    avatar: 'https://images.unsplash.com/photo-1507003211169-0a1dd7228f2d?w=200&q=80',\n  },\n  {\n    content: 'Akhirnya menemukan apartemen impian dengan harga yang sesuai budget. Terima kasih tim yang luar biasa!',\n    name: 'Siti Nurhaliza',\n    role: 'Dokter',\n    avatar: 'https://images.unsplash.com/photo-1494790108377-be9c29b29330?w=200&q=80',\n  },\n  {\n    content: 'Konsultasi investasi properti sangat membantu. Sekarang sudah punya 3 unit untuk passive income.',\n    name: 'Ahmad Wijaya',\n    role: 'Investor',\n    avatar: 'https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?w=200&q=80',\n  },\n]);\n\nconst stats = ref([\n  { value: '500+', label: 'Properti Terjual' },\n  { value: '1000+', label: 'Klien Puas' },\n  { value: '50+', label: 'Kota Terjangkau' },\n  { value: '4.9', label: 'Rating' },\n]);\n</script>\n"],"mappings":"AAiEA,SAASA,GAAG,QAAQ,KAAK;;AAEzB;;;;;;;;IACA,MAAMC,YAAY,GAAGD,GAAG,CAAC,CACvB;MACEE,OAAO,EAAE,gHAAgH;MACzHC,IAAI,EAAE,cAAc;MACpBC,IAAI,EAAE,WAAW;MACjBC,MAAM,EAAE;IACV,CAAC,EACD;MACEH,OAAO,EAAE,wGAAwG;MACjHC,IAAI,EAAE,gBAAgB;MACtBC,IAAI,EAAE,QAAQ;MACdC,MAAM,EAAE;IACV,CAAC,EACD;MACEH,OAAO,EAAE,kGAAkG;MAC3GC,IAAI,EAAE,cAAc;MACpBC,IAAI,EAAE,UAAU;MAChBC,MAAM,EAAE;IACV,CAAC,CACF,CAAC;IAEF,MAAMC,KAAK,GAAGN,GAAG,CAAC,CAChB;MAAEO,KAAK,EAAE,MAAM;MAAEC,KAAK,EAAE;IAAmB,CAAC,EAC5C;MAAED,KAAK,EAAE,OAAO;MAAEC,KAAK,EAAE;IAAa,CAAC,EACvC;MAAED,KAAK,EAAE,KAAK;MAAEC,KAAK