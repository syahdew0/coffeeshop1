{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref, computed, onMounted, watch } from 'vue';\nimport { useRoute } from 'vue-router';\nimport axios from 'axios';\nimport API_ENDPOINTS from '@/config/api';\nexport default {\n  __name: 'ListingDetailPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const LISTING_TYPE = window.LISTING_TYPE || 1;\n    const loading = ref(true);\n    const listing = ref(null);\n    const allListings = ref([]);\n    const inquiry = ref({\n      name: '',\n      phone: '',\n      email: '',\n      message: ''\n    });\n    const features = computed(() => {\n      const result = [{\n        icon: 'fas fa-bed',\n        value: '3',\n        label: 'Kamar Tidur'\n      }, {\n        icon: 'fas fa-bath',\n        value: '2',\n        label: 'Kamar Mandi'\n      }, {\n        icon: 'fas fa-ruler-combined',\n        value: '120 m²',\n        label: 'Luas Bangunan'\n      }, {\n        icon: 'fas fa-layer-group',\n        value: '200 m²',\n        label: 'Luas Tanah'\n      }];\n\n      // Try to get from listing values\n      if (listing.value?.values) {\n        listing.value.values.forEach(val => {\n          const tagLower = (val.tag_name || '').toLowerCase();\n          if (tagLower.includes('kamar tidur') || tagLower.includes('bedroom')) {\n            result[0].value = val.value;\n          } else if (tagLower.includes('kamar mandi') || tagLower.includes('bathroom')) {\n            result[1].value = val.value;\n          } else if (tagLower.includes('luas bangunan') || tagLower.includes('building')) {\n            result[2].value = val.value;\n          } else if (tagLower.includes('luas tanah') || tagLower.includes('land')) {\n            result[3].value = val.value;\n          }\n        });\n      }\n      return result;\n    });\n    const relatedListings = computed(() => {\n      return allListings.value.filter(l => l.post_id !== listing.value?.post_id).slice(0, 3);\n    });\n    const formatPrice = price => {\n      if (!price) return 'Hubungi Kami';\n      const num = parseFloat(price);\n      if (isNaN(num)) return price;\n      return new Intl.NumberFormat('id-ID', {\n        style: 'currency',\n        currency: 'IDR',\n        minimumFractionDigits: 0,\n        maximumFractionDigits: 0\n      }).format(num);\n    };\n    const handleInquiry = () => {\n      console.log('Inquiry submitted:', inquiry.value);\n      alert('Terima kasih! Inquiry Anda telah terkirim.');\n      inquiry.value = {\n        name: '',\n        phone: '',\n        email: '',\n        message: ''\n      };\n    };\n    const fetchListing = async () => {\n      loading.value = true;\n      try {\n        const postId = route.params.id;\n\n        // Try to get from detail endpoint\n        try {\n          const response = await axios.get(API_ENDPOINTS.listingDetail(postId));\n          listing.value = response?.data?.data || response?.data;\n        } catch (err) {\n          // Fallback: fetch all and find by id\n          const response = await axios.get(API_ENDPOINTS.listings, {\n            params: {\n              listing_type: LISTING_TYPE\n            }\n          });\n          const data = response?.data?.data || response?.data || [];\n          allListings.value = data.filter(l => l.listing_type === LISTING_TYPE);\n          listing.value = allListings.value.find(l => l.post_id === parseInt(postId) || l.post_id === postId);\n        }\n\n        // Fetch related listings if not already fetched\n        if (allListings.value.length === 0) {\n          const response = await axios.get(API_ENDPOINTS.listings, {\n            params: {\n              listing_type: LISTING_TYPE\n            }\n          });\n          const data = response?.data?.data || response?.data || [];\n          allListings.value = data.filter(l => l.listing_type === LISTING_TYPE);\n        }\n      } catch (error) {\n        console.error('Failed to fetch listing', error);\n        listing.value = null;\n      } finally {\n        loading.value = false;\n      }\n    };\n    watch(() => route.params.id, () => {\n      if (route.params.id) {\n        fetchListing();\n      }\n    });\n    onMounted(() => {\n      fetchListing();\n    });\n    const __returned__ = {\n      route,\n      LISTING_TYPE,\n      loading,\n      listing,\n      allListings,\n      inquiry,\n      features,\n      relatedListings,\n      formatPrice,\n      handleInquiry,\n      fetchListing,\n      ref,\n      computed,\n      onMounted,\n      watch,\n      get useRoute() {\n        return useRoute;\n      },\n      get axios() {\n        return axios;\n      },\n      get API_ENDPOINTS() {\n        return API_ENDPOINTS;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","useRoute","axios","API_ENDPOINTS","route","LISTING_TYPE","window","loading","listing","allListings","inquiry","name","phone","email","message","features","result","icon","value","label","values","forEach","val","tagLower","tag_name","toLowerCase","includes","relatedListings","filter","l","post_id","slice","formatPrice","price","num","parseFloat","isNaN","Intl","NumberFormat","style","currency","minimumFractionDigits","maximumFractionDigits","format","handleInquiry","console","log","alert","fetchListing","postId","params","id","response","get","listingDetail","data","err","listings","listing_type","find","parseInt","length","error"],"sources":["/Users/itgroup/Library/Mobile Documents/com~apple~CloudDocs/example/new_template_psggroup/compro_new/src/views/ListingDetailPage.vue"],"sourcesContent":["<template>\n  <main class=\"pt-20 bg-slate-50\">\n    <!-- Loading State -->\n    <div v-if=\"loading\" class=\"flex items-center justify-center min-h-screen\">\n      <div class=\"text-center\">\n        <i class=\"fas fa-spinner fa-spin text-4xl text-primary mb-4\"></i>\n        <p class=\"text-slate-600\">Memuat detail properti...</p>\n      </div>\n    </div>\n\n    <!-- Content -->\n    <template v-else-if=\"listing\">\n      <!-- Image Gallery -->\n      <section class=\"bg-white\">\n        <div class=\"max-w-7xl mx-auto\">\n          <div class=\"grid lg:grid-cols-3 gap-2 p-2\">\n            <!-- Main Image -->\n            <div class=\"lg:col-span-2 relative rounded-2xl overflow-hidden h-[400px] lg:h-[500px]\">\n              <img \n                :src=\"listing.post?.thumbnail_url || 'https://images.unsplash.com/photo-1600596542815-ffad4c1539a9?w=800&q=80'\"\n                :alt=\"listing.post?.title\"\n                class=\"w-full h-full object-cover\"\n              />\n              <div class=\"absolute top-4 left-4\">\n                <span class=\"px-4 py-2 rounded-full bg-secondary text-white font-medium\">\n                  {{ listing.listingType?.name || 'Properti' }}\n                </span>\n              </div>\n            </div>\n            <!-- Side Images -->\n            <div class=\"hidden lg:grid grid-rows-2 gap-2\">\n              <div class=\"relative rounded-2xl overflow-hidden\">\n                <img \n                  src=\"https://images.unsplash.com/photo-1560185893-a55cbc8c57e8?w=400&q=80\"\n                  alt=\"Interior\"\n                  class=\"w-full h-full object-cover\"\n                />\n              </div>\n              <div class=\"relative rounded-2xl overflow-hidden\">\n                <img \n                  src=\"https://images.unsplash.com/photo-1560448204-e02f11c3d0e2?w=400&q=80\"\n                  alt=\"Interior\"\n                  class=\"w-full h-full object-cover\"\n                />\n                <button class=\"absolute inset-0 bg-black/50 flex items-center justify-center text-white font-medium hover:bg-black/60 transition-colors\">\n                  <i class=\"fas fa-images mr-2\"></i>\n                  Lihat Semua Foto\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <!-- Main Content -->\n      <section class=\"py-12\">\n        <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n          <div class=\"grid lg:grid-cols-3 gap-8\">\n            <!-- Left Column - Details -->\n            <div class=\"lg:col-span-2 space-y-8\">\n              <!-- Header -->\n              <div class=\"bg-white rounded-2xl p-8\">\n                <div class=\"flex flex-wrap items-center gap-2 mb-4\">\n                  <span \n                    v-for=\"cat in (listing.post?.categories || [])\"\n                    :key=\"cat.id\"\n                    class=\"px-3 py-1 rounded-full bg-primary/10 text-primary text-sm font-medium\"\n                  >\n                    {{ cat.name }}\n                  </span>\n                </div>\n                <h1 class=\"text-3xl font-bold text-primary mb-4\">\n                  {{ listing.post?.title || 'Detail Properti' }}\n                </h1>\n                <div class=\"flex items-center gap-2 text-slate-600 mb-6\">\n                  <i class=\"fas fa-map-marker-alt text-secondary\"></i>\n                  <span>{{ [listing.kecamatan, listing.kabupaten, listing.provinsi].filter(Boolean).join(', ') }}</span>\n                </div>\n                <div class=\"text-3xl font-bold text-secondary\">\n                  {{ formatPrice(listing.price) }}\n                </div>\n              </div>\n\n              <!-- Features -->\n              <div class=\"bg-white rounded-2xl p-8\">\n                <h2 class=\"text-xl font-bold text-primary mb-6\">Spesifikasi</h2>\n                <div class=\"grid grid-cols-2 md:grid-cols-4 gap-6\">\n                  <div \n                    v-for=\"feature in features\"\n                    :key=\"feature.label\"\n                    class=\"text-center p-4 rounded-xl bg-accent/30\"\n                  >\n                    <i :class=\"feature.icon\" class=\"text-2xl text-secondary mb-2\"></i>\n                    <div class=\"font-bold text-primary\">{{ feature.value }}</div>\n                    <div class=\"text-sm text-slate-500\">{{ feature.label }}</div>\n                  </div>\n                </div>\n              </div>\n\n              <!-- Description -->\n              <div class=\"bg-white rounded-2xl p-8\">\n                <h2 class=\"text-xl font-bold text-primary mb-6\">Deskripsi</h2>\n                <div \n                  class=\"prose prose-slate max-w-none\"\n                  v-html=\"listing.post?.content || listing.post?.excerpt || 'Deskripsi properti belum tersedia.'\"\n                ></div>\n              </div>\n\n              <!-- Additional Info -->\n              <div v-if=\"listing.values && listing.values.length > 0\" class=\"bg-white rounded-2xl p-8\">\n                <h2 class=\"text-xl font-bold text-primary mb-6\">Informasi Tambahan</h2>\n                <div class=\"grid md:grid-cols-2 gap-4\">\n                  <div \n                    v-for=\"val in listing.values\"\n                    :key=\"val.tag_name\"\n                    class=\"flex items-center justify-between py-3 border-b border-slate-100\"\n                  >\n                    <span class=\"text-slate-600\">{{ val.tag_name }}</span>\n                    <span class=\"font-medium text-primary\">{{ val.value }}</span>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <!-- Right Column - Contact Card -->\n            <div class=\"lg:col-span-1\">\n              <div class=\"bg-white rounded-2xl p-8 sticky top-28\">\n                <h3 class=\"text-xl font-bold text-primary mb-6\">Hubungi Kami</h3>\n                <form @submit.prevent=\"handleInquiry\" class=\"space-y-4\">\n                  <div>\n                    <input \n                      v-model=\"inquiry.name\"\n                      type=\"text\" \n                      placeholder=\"Nama Lengkap\"\n                      class=\"input-field\"\n                      required\n                    />\n                  </div>\n                  <div>\n                    <input \n                      v-model=\"inquiry.phone\"\n                      type=\"tel\" \n                      placeholder=\"No. Telepon\"\n                      class=\"input-field\"\n                      required\n                    />\n                  </div>\n                  <div>\n                    <input \n                      v-model=\"inquiry.email\"\n                      type=\"email\" \n                      placeholder=\"Email\"\n                      class=\"input-field\"\n                      required\n                    />\n                  </div>\n                  <div>\n                    <textarea \n                      v-model=\"inquiry.message\"\n                      rows=\"4\" \n                      placeholder=\"Pesan Anda...\"\n                      class=\"textarea-field\"\n                    ></textarea>\n                  </div>\n                  <button type=\"submit\" class=\"btn-primary w-full\">\n                    <i class=\"fas fa-paper-plane\"></i>\n                    Kirim Inquiry\n                  </button>\n                </form>\n\n                <div class=\"mt-6 pt-6 border-t border-slate-100\">\n                  <div class=\"text-center text-slate-500 mb-4\">atau hubungi langsung</div>\n                  <div class=\"flex gap-3\">\n                    <a \n                      href=\"https://wa.me/6281234567890\"\n                      target=\"_blank\"\n                      class=\"flex-1 btn-secondary justify-center text-green-600 border-green-600 hover:bg-green-600 hover:text-white\"\n                    >\n                      <i class=\"fab fa-whatsapp\"></i>\n                      WhatsApp\n                    </a>\n                    <a \n                      href=\"tel:+6281234567890\"\n                      class=\"flex-1 btn-secondary justify-center\"\n                    >\n                      <i class=\"fas fa-phone\"></i>\n                      Telepon\n                    </a>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <!-- Related Listings -->\n      <section v-if=\"relatedListings.length > 0\" class=\"py-12 bg-white\">\n        <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n          <h2 class=\"text-2xl font-bold text-primary mb-8\">Properti Serupa</h2>\n          <div class=\"grid md:grid-cols-3 gap-8\">\n            <div \n              v-for=\"item in relatedListings\"\n              :key=\"item.post_id\"\n              class=\"property-card group\"\n            >\n              <div class=\"property-card-image\">\n                <img \n                  :src=\"item.post?.thumbnail_url || 'https://images.unsplash.com/photo-1560518883-ce09059eeffa?w=400&q=80'\"\n                  :alt=\"item.post?.title\"\n                  class=\"w-full h-full object-cover group-hover:scale-110 transition-transform duration-500\"\n                />\n              </div>\n              <div class=\"property-card-content\">\n                <h3 class=\"property-card-title\">{{ item.post?.title }}</h3>\n                <div class=\"property-card-location\">\n                  <i class=\"fas fa-map-marker-alt text-secondary\"></i>\n                  <span>{{ item.kecamatan || item.kabupaten }}</span>\n                </div>\n                <div class=\"flex items-center justify-between\">\n                  <div class=\"property-card-price\">{{ formatPrice(item.price) }}</div>\n                  <router-link :to=\"`/listing/${item.post_id}`\" class=\"text-primary hover:text-secondary font-medium\">\n                    Detail <i class=\"fas fa-arrow-right ml-1\"></i>\n                  </router-link>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n    </template>\n\n    <!-- Not Found -->\n    <div v-else class=\"flex items-center justify-center min-h-screen\">\n      <div class=\"text-center\">\n        <div class=\"w-24 h-24 mx-auto mb-6 rounded-full bg-slate-100 flex items-center justify-center\">\n          <i class=\"fas fa-home text-4xl text-slate-400\"></i>\n        </div>\n        <h2 class=\"text-2xl font-bold text-slate-700 mb-2\">Properti Tidak Ditemukan</h2>\n        <p class=\"text-slate-500 mb-6\">Properti yang Anda cari tidak tersedia</p>\n        <router-link to=\"/listing\" class=\"btn-primary\">\n          <i class=\"fas fa-arrow-left\"></i>\n          Kembali ke Listing\n        </router-link>\n      </div>\n    </div>\n  </main>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted, watch } from 'vue';\nimport { useRoute } from 'vue-router';\nimport axios from 'axios';\nimport API_ENDPOINTS from '@/config/api';\n\nconst route = useRoute();\nconst LISTING_TYPE = window.LISTING_TYPE || 1;\n\nconst loading = ref(true);\nconst listing = ref(null);\nconst allListings = ref([]);\n\nconst inquiry = ref({\n  name: '',\n  phone: '',\n  email: '',\n  message: '',\n});\n\nconst features = computed(() => {\n  const result = [\n    { icon: 'fas fa-bed', value: '3', label: 'Kamar Tidur' },\n    { icon: 'fas fa-bath', value: '2', label: 'Kamar Mandi' },\n    { icon: 'fas fa-ruler-combined', value: '120 m²', label: 'Luas Bangunan' },\n    { icon: 'fas fa-layer-group', value: '200 m²', label: 'Luas Tanah' },\n  ];\n  \n  // Try to get from listing values\n  if (listing.value?.values) {\n    listing.value.values.forEach(val => {\n      const tagLower = (val.tag_name || '').toLowerCase();\n